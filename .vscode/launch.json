            "args": [
                "${workspaceRoot}/test/createVersion_spec.js"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "FPR Tests",
            "env": {
                "DEBUG": "true"
            },
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
